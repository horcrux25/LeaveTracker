<Window x:Class="LeaveTracker.Calendar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LeaveTracker"
        mc:Ignorable="d"
        Title="Calendar" Height="500" Width="500"
        Closing="CalendarWindow_Closing">
    <Grid Margin="0,0,0,-6">
        <Label Content="Name :" HorizontalAlignment="Left" Margin="10,42,0,0" VerticalAlignment="Top" Width="50"/>
        <TextBlock Name="CalName" HorizontalAlignment="Left" Margin="65,47,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="220"/>
        <Label Content="Leave Tracker" HorizontalAlignment="Center" Height="28" Margin="0,10,0,0" VerticalAlignment="Top" Width="98" FontSize="14" FontWeight="Medium" FontFamily="KacstBook"/>
        <Button Name="GoBack" Content="Go Back" HorizontalAlignment="Center" Margin="0,424,0,0" VerticalAlignment="Top" Click="GoBack_Click" Height="29" Width="90"/>

    </Grid>
</Window>
